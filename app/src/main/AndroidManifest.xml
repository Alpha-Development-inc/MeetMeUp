<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.meetmeup">

    <application
        android:name=".MeetMeUpApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".SignupActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".GroupCreateActivity" />
        <activity android:name=".CreateNewActivity" />
        <activity android:name=".CreateFrame" />
        <activity android:name=".ActivityFrame" />
        <activity android:name=".MySchedule" />
        <activity android:name=".MyAccount" />
        <activity android:name=".ScheduleEdit"/>
        <activity android:name=".MyGroupsActivity"/>
    </application>

</manifest>